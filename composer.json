{
    "name": "maitre-hibou/tictactoe",
    "description": "Algorythm test for Gogaille job.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "files": [
            "./src/andTheWinnerIs.php"
        ],
        "psr-4": {
            "MaitreHibou\\TicTacToe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaitreHibou\\TicTacToe\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Arnaud P",
            "email": "arnaud@xpressive.io"
        }
    ],
    "require": {
        "symfony/console": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.0",
        "friendsofphp/php-cs-fixer": "^3.8",
        "vimeo/psalm": "^4.22"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
